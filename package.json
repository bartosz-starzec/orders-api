{
    "name": "orders-api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "packageManager": "pnpm@8.6.10",
    "scripts": {
        "start:dev": "nest start --watch",
        "build": "nest build && echo 'Build completed'",
        "start": "node dist/src/main.js",
        "start:prod": "node dist/src/main.js",
        "seed": "ts-node -r tsconfig-paths/register src/database/seed/seed.ts",
        "test": "jest",
        "typeorm": "typeorm-ts-node-commonjs --dataSource data-source.ts",
        "migration:generate": "npm run typeorm migration:generate src/database/migrations/InitMigration",
        "migration:run": "npm run typeorm migration:run",
        "migration:revert": "npm run typeorm migration:revert",
        "create:migration": "ts-node -r tsconfig-paths/register scripts/create-migration.ts"
    },
    "dependencies": {
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.1",
        "@nestjs/jwt": "^11.0.1",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/swagger": "^11.2.3",
        "@nestjs/typeorm": "^11.0.0",
        "@rushstack/eslint-patch": "^1.15.0",
        "@types/winston": "^2.4.4",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "dayjs": "^1.11.19",
        "dotenv": "16.4.5",
        "lru-cache": "^11.2.2",
        "mysql2": "^3.15.3",
        "nestjs-zod": "^5.0.1",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.18.3",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@types/express": "^5.0.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.10.7",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.47.0",
        "@typescript-eslint/parser": "^8.47.0",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "jest": "^30.0.0",
        "prettier": "^3.4.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.4.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.27",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
